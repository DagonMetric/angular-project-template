{
  "$schema": "./node_modules/@dagonmetric/angular-build/src/schemas/schema.json#",
  "libs": [{
    "extends": "ngb:lib-default",
      "name": "repo-name",
      "root": "modules/repo-name",
      "outputPath": "dist/packages/repo-name",
      "libraryName": "<library-name>",
      "envOverrides": {
        "prod": {
          "copy": [
            "../../README.md",
            "../../LICENSE"
          ],
          "bundles": true,
          "packageJsonCopy": true,
          "banner": "../banner.txt"
        }
      }
    }
  ]
}
